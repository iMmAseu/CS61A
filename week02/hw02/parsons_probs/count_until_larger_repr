def count_until_larger(num): #0given
!BLANK = 0  #1given #blankcount
!BLANK = num % 10  #1given #blanknow_number
while !BLANK:  #1given #blanknum != 0
!BLANK += !BLANK  #2given #blankcount #blank1
!BLANK //= !BLANK  #2given #blanknum #blank10
if num % 10 > !BLANK:  #2given #blanknow_number
return !BLANK  #3given #blankcount
return !BLANK  #1given #blank-1
print('DEBUG:', !BLANK)
print('DEBUG:', !BLANK)
